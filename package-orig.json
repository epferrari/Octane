{
	"name": "octane",
	"version": "0.1.0",
	"description": "MVC UI-centric app framework with 2-way data binding",
	"main": "Gruntfile.js",
	"dependencies": {
		"bluebird": "^2.9.25",
		"classlist": "^1.0.1",
		"fastclick": "^1.0.6",
		"history.js": "latest",
		"html2canvas": "^0.5.0-alpha2",
		"lodash": "^3.9.1",
		"modernizr": "^3.0.0-alpha.4",
		"velocity-animate": "^1.2.2",
		"velocity-ui-pack": "^1.2.2"
	},
	"devDependencies": {
		"autoprefixer": "^5.0.0",
		"autoprefixer-core": "^5.0.0",
		"bower": "^1.3.12",
		"browserify": "^10.2.0",
		"browserify-css": "^0.6.1",
		"coffee-script": "^1.9.2",
		"eslint": "^0.21.2",
		"file": "^0.2.2",
		"filesize": "^3.1.2",
		"grunt": "latest",
		"grunt-autoprefixer": "^2.1.0",
		"grunt-bower-concat": "^0.4.0",
		"grunt-contrib-clean": "^0.6.0",
		"grunt-contrib-concat": "^0.5.0",
		"grunt-contrib-copy": "^0.7.0",
		"grunt-contrib-cssmin": "^0.10.0",
		"grunt-contrib-jshint": "^0.10.0",
		"grunt-contrib-sass": "^0.8.1",
		"grunt-contrib-uglify": "^0.6.0",
		"grunt-contrib-watch": "^0.6.1",
		"grunt-postcss": "^0.2.0",
		"gulp-concat": "^2.5.2",
		"node-sass": "^3.1.2",
		"postcss": "^4.0.1",
		"requirejs": "^2.1.15",
		"system": "^0.1.0",
		"uglify-js": "^2.4.23"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean:build": "rm -rf ./build/*",
		"build:browserify": "mkdir -p ./build/assets/js && browserify -s 'octane' ./src/octane.js > ./build/assets/js/octane.js",
		"build:sass": "mkdir -p ./build/assets/css && node-sass ./src/assets/scss/octane.scss ./build/assets/css/octane.css",
		"build:css": "autoprefixer ./build/assets/css/octane.css",
		"build:html": "cp ./src/index.html ./build/index.html",
		"build:grunt": "grunt build",
		"prebuild": "eslint ./src/octane.js && npm run clean:build",
		"build": "npm run build:browserify && npm run build:sass && npm run build:css && npm run build:html",
		"postbuild": "npm run dist",
		"predist": "rm -rf ./dist/*",
		"dist": "mkdir -p ./dist/assets/js && uglifyjs --compress --mangle -o ./dist/assets/js/octane-min.js ./build/assets/js/octane.js"
	},
	"author": "Ethan Ferrari",
	"license": "MIT"
}
